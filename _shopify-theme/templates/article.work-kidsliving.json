    "16540104123512b9a3": {
      "type": "hero",
      "settings": {
        "pre_title": "Portfolio of Work",
        "title": "KidsLiving",
        "paragraph": "<p>Est. 2008 in Cape Town, now a leading South African baby and kids store, offering an extensive range of imported and locally manufactured high-end products. We have worked with Kids Living since 2017 as their ecommerce success partners, covering all aspects from inventory management, digital marketing, web development to complete custom API solutions.<\/p>",
        "list_title": "What we did:",
        "list": "<p>Web Development<\/p><p>Custom Features Development<\/p><p>Custom App Integrations<\/p><p>Monthly SEO & Site maintenance support<\/p>",
        "image": "shopify:\/\/shop_images\/627bcf588c162da9f0358ef0_Group_25201247_2520_3_-min-p-800_1.png"
      }
    },
